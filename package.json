  "homepage": "https://github.com/TheLucifurry/fast-int-set#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLucifurry/fast-int-set.git"
  },
  "bugs": {
    "url": "https://github.com/TheLucifurry/fast-int-set/issues"
  },
  "keywords": [
    "fast",
    "set",
    "bit",
