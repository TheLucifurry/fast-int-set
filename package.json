{
  "name": "fast-int-set",
  "version": "0.0.19",
  "description": "",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "dev": "node build.js --dev",
    "format": "prettier-eslint \"src/**/*.ts\"",
    "build": "rollup --config rollup.config.ts",
    "benchmark": "node ./benchmark/index.js",
    "test": "jest",
    "test_watch": "jest --watch",
    "test_coverage": "jest --coverage",
    "version:patch": "npm version patch --force",
    "version:minor": "npm version minor --force",
    "version:major": "npm version major --force",
    "prepare": "npm run build",
    "postversion": "git push && npm publish"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLucifurry/fast-int-set.git"
  },
  "keywords": [
    "fast",
    "set",
    "bit",
    "mask",
    "bitwise"
  ],
  "author": "Lucifurry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheLucifurry/fast-int-set/issues"
  },
  "homepage": "https://github.com/TheLucifurry/fast-int-set#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "benny": "^3.7.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.4",
    "prettier-eslint-cli": "^5.0.1",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
