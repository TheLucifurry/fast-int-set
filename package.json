{
  "name": "fast-int-set",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/fast-int-set.umd.cjs",
  "module": "./dist/fast-int-set.js",
  "exports": {
    ".": {
      "import": "./dist/fast-int-set.js",
      "require": "./dist/fast-int-set.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "benchmark": "node ./benchmark/index.js",
    "test": "jest",
    "test_watch": "jest --watch",
    "test_coverage": "jest --coverage",
    "version:patch": "npm version patch --force",
    "version:minor": "npm version minor --force",
    "version:major": "npm version major --force",
    "prepare": "npm run build",
    "postversion": "git push && npm publish"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLucifurry/fast-int-set.git"
  },
  "keywords": [
    "fast",
    "set",
    "bit",
    "mask",
    "bitwise"
  ],
  "author": "Lucifurry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheLucifurry/fast-int-set/issues"
  },
  "homepage": "https://github.com/TheLucifurry/fast-int-set#readme",
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/jest": "^29.5.0",
    "benny": "^3.7.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "terser": "^5.16.9",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0"
  }
}